CREATE TABLE Contents (
	Literature int NOT NULL,
	Code text NOT NULL,
	"Name" text,
	Parent text,
	IsNoDef int NOT NULL,
	DateLoad varchar(25),
	Secret int,
	AltName text,
	UUID text,
	UUIDPar text,
	PRIMARY KEY (Literature,Code)
);
CREATE TABLE Definition (
	Literature int,
	"Name" text NOT NULL,
	Homo int,
	Define text,
	Dash text,
	Sost int,
	"Content" text,
	"Rank" int,
	Rank2 text,
	Small text,
	Short text,
	IsShort int,
	England text,
	History text,
	DateLoad varchar(25),
	Secret int,
	NameFind text,
	NameSort text,
	DefineEdit text,
	Rub text,
	UUID text,
	UUIDPar text,
	UUIDRef text
);
CREATE TABLE Literature (
	Code int NOT NULL,
	"Name" text NOT NULL,
	CodeFirst int,
	CodeSecond int,
	CodeMain int,
	Short text,
	Dict int,
	NRef int,
	Tema text,
	Signif text,
	LitType int,
	Lib int,
	LibIndex text,
	Secret int,
	IsWork int,
	IsImage int,
	DateLoad varchar(25),
	File_Code text,
	Define text,
	Cover bytea,
	FileImg text,
	FileDoc text,
	FileTxt text,
	Queue int,
	IsDefinition int,
	IsSetView int,
	IsRank int,
	IsContent int,
	IsAlpha int,
	IsNameSort int,
	Annul int,
	TypeDict int,
	Inc int,
	IsNewLine int,
	PRIMARY KEY (Code)
);
CREATE TABLE Picture (
	Code text NOT NULL,
	Literature int NOT NULL,
	Term text,
	"Name" text,
	Picture bytea,
	"Type" text,
	Percent int,
	PercentWord int,
	Secret int,
	PRIMARY KEY (Code)
);
CREATE TABLE RubrBook (
	Code text NOT NULL,
	"Name" text,
	Parent text,
	PRIMARY KEY (Code)
);
CREATE TABLE Shortened (
	Code int NOT NULL,
	Short text NOT NULL,
	"Name" text NOT NULL,
	DateLoad varchar(25),
	Secret int,
	PRIMARY KEY (Code,"Name")
);
CREATE TABLE Signif (
	Code text NOT NULL,
	"Name" text,
	Parent text,
	PRIMARY KEY (Code)
);
CREATE TABLE "Storage" (
	Code text NOT NULL,
	"Name" text NOT NULL,
	PRIMARY KEY (Code)
);
CREATE TABLE TypeDict (
	Code int,
	"Name" text,
	Titl text
);
CREATE TABLE WordMain (
	NumWord int,
	NameFind text,
	WordMain text,
	Base int,
	Exc int
);
