<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/pages/protected/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>user_list</from-outcome>
            <to-view-id>/pages/protected/user/userslist.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dictionary</from-outcome>
            <to-view-id>/pages/protected/dictionary/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/protected/user/userslist.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/pages/protected/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dictionary</from-outcome>
            <to-view-id>/pages/protected/dictionary/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/protected/user/adduser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/protected/user/adduser.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/pages/protected/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/protected/user/failed.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/pages/protected/dictionary/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/pages/protected/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>